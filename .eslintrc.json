{
 "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "mocha": true
  },
  "extends": ["eslint:recommended", "google"],
  "rules": {
    "no-console": "off",
    "require-jsdoc": "off",
    "no-invalid-this": "off"
  },
  "plugins": [
    "react"
  ]
}